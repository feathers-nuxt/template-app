{
  "name": "<%= name %>",
  "description": "<%= description %>",
  "repository": {
    "url": "<%= username %>/<%= name %>",
    "type": "git"
  },
  "author": "<%= username %> <<%= email %>>",
  "private": true,
  "version": "0.0.0",
  "main": "./dist/server/main.js",
  "scripts": {
    "migrate": "npx f3 migrate",
    "seed": "npx f3 seed", 
    "dev": "npx f3 dev --experimental-repl-await",
    "build": "npx f3 build",
    "build:server": "npx f3 build-server",
    "build:client": "npx f3 build-client",
    "analyze": "npx yarn build:analyze && npx yarn start:analyzer",
    "build:analyze": "npx f3 build-client -analyze",
    "start:analyzer": "npx webpack-bundle-analyzer .nuxt/stats/client.json",
    "test": "npx cucumber-js --require-module  livescript --require 'tests/features/**/*.ls' tests/features",
    "start": "cross-env NODE_ENV=production node ./dist/server/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@casl/ability": "^2.5.1",
    "@casl/vue": "^0.5.0",
    "@feathers-nuxt/cli": "^1.1.8",
    "@feathersjs/authentication": "^2.1.13",
    "@feathersjs/authentication-client": "^1.0.8",
    "@feathersjs/authentication-jwt": "^2.0.7",
    "@feathersjs/authentication-local": "^1.2.7",
    "@feathersjs/configuration": "^2.0.4",
    "@feathersjs/errors": "^3.3.4",
    "@feathersjs/express": "^1.2.7",
    "@feathersjs/feathers": "^3.2.3",
    "@feathersjs/rest-client": "^1.4.5",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "axios": "^0.18.0",
    "compression": "^1.7.3",
    "conditional-middleware": "^0.2.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dauria": "^2.0.0",
    "feathers-hooks-common": "^4.17.14",
    <% if(cache) { %>"feathers-hooks-rediscache": "^1.1.3", <% } %>
    "feathers-hooks-validator": "github:kharhys/feathers-hooks-validator",
    "feathers-logger": "^0.3.2",    
    <% if(database == 'memory') { %>"feathers-memory": "^2.1.3", <% } %>
    <% if(database == 'embedded') { %>"feathers-nedb": "^3.1.0", <% } %>
    <% if(database == 'sql') { %>"feathers-sequelize": "^3.1.0", <% } %>
    "form-data": "^2.3.3",
    "helmet": "^3.15.0",
    "into-stream": "^4.0.0",
    "iview": "^3.1.4",    
    <% if(sequelize_dialect == 'mysql') { %>"mysql": "^2.15.0", <% } %> 
    <% if(database == 'file') { %>"nedb": "^1.8.0", <% } %>    
    <% if(resque) { %>"node-resque": "^5.3.0", <% } %>
    "nuxt": "^2.2.0",
    "nuxt-robots-module": "^1.3.0",
    <% if(sequelize_dialect == 'postgresql') { %>"pg": "^7.4.2",  <% } %>
    <% if(sequelize_dialect == 'postgresql') { %>"pg-hstore": "^2.3.2",  <% } %>
    <% if(database == 'sql') { %>"sequelize": "^4.37.6", <% } %>
    "passport-custom": "^1.0.5",
    "pretty-error": "^2.1.1",
    "serve-favicon": "^2.5.0",
    <% if(sequelize_dialect == 'sqlite') { %>"sqlite3": "^4.0.0", <% } %>
    <% if(sequelize_dialect == 'mssql') { %>"tedious": "^2.3.1", <% } %>
    "vue-media-query-mixin": "^0.1.0",
    "vuex-router-sync": "^5.0.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "cucumber": "^5.0.3",
    "cucumber-mink": "^2.1.0",
    "signale": "^1.3.0",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
